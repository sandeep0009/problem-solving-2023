
#include <bits/stdc++.h>
using namespace std;

int triangle(int a,int b, int c)
{
    if(a==b && b==c && a==c)
    {
        cout<<"equvilateral triangle";
    }
    else if(a==b && b==c && a!=c)
    {

        cout<<"scalene triangle";
    }
    else{
        cout<<"isoseles triangle";
    }
}


int main()
{
    int a,b,c;
    cin>>a>>b>>c;
    triangle(a,b,c);
}

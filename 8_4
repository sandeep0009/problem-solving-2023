#include<iostream>
using namespace std;
int main()
{
///1 2 3 4 5 6 7
    int n,count1=0,count2=0;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {

        cin>>a[i];
    }

    int mid=n/2;
    for(int i=0;i<mid;i++)
    {

        count1=count1+a[i];
        count2=count2+a[mid+i+1];

    }

    if(count1==count2)
    {

        cout<<"BALANCE";
    }
    else
    {

        cout<<"UNBALANCE";
    }

}
